<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="urn://x-artefacts-uishc.domrf.ru/receipt-rns/1.0.9"
           xmlns:com="urn://x-artefacts-uishc.domrf.ru/receipt-rns/commons/1.0.9"
           xmlns:smev="urn://x-artefacts-smev-gov-ru/supplementary/commons/1.3.0"
           targetNamespace="urn://x-artefacts-uishc.domrf.ru/receipt-rns/1.0.9"
           elementFormDefault="qualified">
    <xs:import namespace="urn://x-artefacts-uishc.domrf.ru/receipt-rns/commons/1.0.9"
               schemaLocation="./commons/receipt-rns-common-types-1.0.9.xsd"/>
    <xs:import namespace="urn://x-artefacts-smev-gov-ru/supplementary/commons/1.3.0"
               schemaLocation="./commons/smev-supplementary-commons-1.3.0.xsd"/>
    <!-- Основные элементы Вида сведений -->
    <xs:element name="Request">
        <xs:annotation>
            <xs:documentation>Запрос на размещение сведений нового документа РНC (Разрешение на строительство) или на
                размещение сведений документа РНC взамен (Разрешение на строительство)
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Тип Запрос с бизнес-данными</xs:documentation>
            </xs:annotation>
            <xs:choice>
                <xs:element name="ReceiptListConstruction" type="tns:ReceiptListConstructionType">
                    <xs:annotation>
                        <xs:documentation>Данные запроса на получение списка РНС(Разрешение на строительство)
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="ReceiptConstruction" type="tns:ReceiptConstructionType">
                    <xs:annotation>
                        <xs:documentation>Данные запроса на получение сведений о РНС(Разрешение на строительство)
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="Response" type="tns:ResponseType">
        <xs:annotation>
            <xs:documentation>Ответ на запрос о получении сведений о РНС(Разрешение на строительство)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!--Сведения о запросе-->

    <xs:complexType name="ReceiptListConstructionType">
        <xs:annotation>
            <xs:documentation>Бизнес-данные запроса на получение списка РНС(Разрешение на строительство)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="RecipientInfo" type="com:RecipientInfoType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сведения о получателе (застройщике)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitNumber" type="com:stringMaxLength-255" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitID" type="com:stringMaxLength-10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitDateFrom" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата выдачи разрешения на строительство, с</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitDateTo" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата выдачи разрешения на строительство, по</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExpireDateFrom" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата окончания действия разрешения, с</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExpireDateTo" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата окончания действия разрешения, по</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IssueOrgan" type="com:OrganizationInfoType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Организация, выдавшая разрешение</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RequestObjectInfo" type="tns:RequestObjectShortInfoType" minOccurs="0"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сведения об объектах</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ReceiptConstructionType">
        <xs:annotation>
            <xs:documentation>Бизнес-данные запроса на получение сведений о РНС(Разрешение на строительство)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="RecipientInfo" type="com:RecipientInfoType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сведения о получателе (застройщике)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitNumber" type="com:stringMaxLength-255" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitID" type="com:stringMaxLength-10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата выдачи разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExpireDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата окончания действия разрешения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IssueOrgan" type="com:OrganizationInfoType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Организация, выдавшая разрешение</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RequestObjectInfo" type="tns:RequestObjectShortInfoType" maxOccurs="unbounded"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сведения об объектах</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--Сведения об ответе-->
    <xs:complexType name="ResponseType">
        <xs:annotation>
            <xs:documentation>Данные ответа на запрос получения сведений о РНС(Разрешение на строительство)
            </xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="ResponseListConstruction" type="tns:ResponseConstructionShortInfoType"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Данные ответа на получение списка РНС(Разрешение на строительство)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ResponseConstruction" type="tns:ResponseConstructionType">
                <xs:annotation>
                    <xs:documentation>Данные ответа на получение сведений о РНС(Разрешение на строительство)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="ResponseConstructionType">
        <xs:annotation>
            <xs:documentation>Подробная информация о РНС</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="RegTime" type="xs:dateTime" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата размещения РНС в ЕО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ChangesConstPermit" type="tns:ChangesConstPermitType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сведения о внесении изменений в разрешение на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Construction" type="tns:ConstructionType">
                <xs:annotation>
                    <xs:documentation>Информация о выданном разрешении на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ChangesConstPermitType">
        <xs:annotation>
            <xs:documentation>Сведения о внесении изменений</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="PrevConstPermitID" type="com:stringMaxLength-10">
                <xs:annotation>
                    <xs:documentation>Идентификатор предыдущего Разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PrevConstPermitNumber" type="com:stringMaxLength-45">
                <xs:annotation>
                    <xs:documentation>Номер предыдущего Разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PrevConstPermitDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата выдачи предыдущего Разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ChangesDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата внесения изменений</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ReasonChanges" type="com:RefBookType" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Значение справочника «Причин внесения изменений»</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BasisChanges" type="com:string-4000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Основание внесений изменений (на основании какого документа)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExtendedTo" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Продлить до (дата продления разрешения на строительство)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ResponseConstructionShortInfoType">
        <xs:annotation>
            <xs:documentation>Ответ на запрос списка сведений о документе с дополнительными атрибутами версий документа
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="VersionInfo" type="tns:ConstructionVersionInfoType">
                <xs:annotation>
                    <xs:documentation>Сведения о документе</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PreviousVersionInfo" type="tns:ConstructionVersionInfoType" minOccurs="0"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сведения о предыдущих версиях документа</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ConstructionVersionInfoType">
        <xs:annotation>
            <xs:documentation>Сведения о версии документа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ConstPermitID" type="com:stringMaxLength-10">
                <xs:annotation>
                    <xs:documentation>Идентификатор разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата выдачи разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitNumber" type="com:stringMaxLength-255">
                <xs:annotation>
                    <xs:documentation>Сведения о документе</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExpireDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата окончания действия разрешения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ChangesDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата внесения изменений</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ReasonChanges" type="com:RefBookType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Значение справочника «Причин внесения изменений»</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IssueOrgan" type="com:OrganizationInfoType">
                <xs:annotation>
                    <xs:documentation>Сведения об органе, организации, выдавшей (его) разрешение на строительство
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RecipientInfo" type="com:RecipientInfoType">
                <xs:annotation>
                    <xs:documentation>Сведения о получателе (застройщике)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RecipientID" type="com:stringMaxLength-10">
                <xs:annotation>
                    <xs:documentation>Идентификатор получателя (Застройщика)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectShortInfo" type="tns:ObjectShortInfoType" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сведения об объектах</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StatusConstruction" type="tns:StatusConstructionType">
                <xs:annotation>
                    <xs:documentation>Статус разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OldId" type="com:stringMaxLength-10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Старый идентификатор РНС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OldVersionId" type="com:stringMaxLength-10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Старый идентификатор версии РНС</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RequestObjectShortInfoType">
        <xs:annotation>
            <xs:documentation>Краткие сведения об объектах</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ObjectName" type="com:stringMaxLength-3500" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование объекта (этапа) в составе проекта строительства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectID" type="com:stringMaxLength-10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор объекта (этапа) в составе проекта строительства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectBusinessID" type="com:ObjectBusinessIDType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Бизнес-идентификатор объекта (этапа) в составе проекта строительства
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ObjectShortInfoType">
        <xs:annotation>
            <xs:documentation>Краткие сведения об объектах</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ObjectName" type="com:stringMaxLength-3500">
                <xs:annotation>
                    <xs:documentation>Наименование объекта (этапа) в составе проекта строительства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectID" type="com:stringMaxLength-10">
                <xs:annotation>
                    <xs:documentation>Идентификатор объекта (этапа) в составе проекта строительства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectBusinessID" type="com:ObjectBusinessIDType">
                <xs:annotation>
                    <xs:documentation>Бизнес-идентификатор объекта (этапа) в составе проекта строительства
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--Основные типы сведений о документе РНС-->
    <xs:complexType name="ConstructionType">
        <xs:annotation>
            <xs:documentation>Информация о выданном разрешении на строительство</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="RecipientInfo" type="com:RecipientInfoType">
                <xs:annotation>
                    <xs:documentation>Сведения о получателе (застройщике)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitNumber" type="com:stringMaxLength-255">
                <xs:annotation>
                    <xs:documentation>Номер разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstGovernmentOrderId" type="com:GovernmentOrder" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Идентификатор номера приказа, в соответсвии с которым выдается РНС
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstCadastralDistrict" type="com:stringMaxLength-10" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Кадастровый округ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstCadastralArea" type="com:stringMaxLength-10" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Кадастровый район</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitID" type="com:stringMaxLength-10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermitDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата выдачи разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExpireDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата окончания действия разрешения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IssueOrgan" type="com:OrganizationInfoType">
                <xs:annotation>
                    <xs:documentation>Организация, выдавшая разрешение</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IssuePersonPosition" type="com:stringMaxLength-500">
                <xs:annotation>
                    <xs:documentation>Должность уполномоченного сотрудника органа, организации, осуществляющего выдачу
                        разрешения на строительство
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IssuePerson" type="com:FIOType">
                <xs:annotation>
                    <xs:documentation>Сведения об уполномоченном сотруднике органа, организации, осуществляющего выдачу
                        разрешения на строительство
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstructionKind" type="com:RefBookType">
                <xs:annotation>
                    <xs:documentation>Значение справочника «Вид строительства»</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstPermissionType" type="com:RefBookType">
                <xs:annotation>
                    <xs:documentation>Значение справочника «Вид разрешения»</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectNameProjectDoc" type="com:stringMaxLength-3500">
                <xs:annotation>
                    <xs:documentation>Наименование объекта (этапа) в соответствии с проектной документацией
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NumberObjects" type="com:stringMaxLength-5" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Количество объектов (этапов)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NumberLongObjects" type="com:stringMaxLength-5" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Количество линейных объектов</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectInfo" type="tns:ObjectInfoType" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сведения об объекте</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StatusConstruction" type="tns:StatusConstructionType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Статус разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Scans" type="tns:FileType" maxOccurs="unbounded" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Скан-копии РНС</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ObjectInfoType">
        <xs:annotation>
            <xs:documentation>Сведения об объекте</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ObjectName" type="com:stringMaxLength-3500">
                <xs:annotation>
                    <xs:documentation>Наименование объекта (этапа) в составе проекта строительства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectID" type="com:stringMaxLength-10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор объекта из разрешения на строительство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectBusinessID" type="com:ObjectBusinessIDType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Бизнес-идентификатор объекта (этапа) в составе проекта строительства
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectAddress" type="com:AddressFullType">
                <xs:annotation>
                    <xs:documentation>Адрес (местоположение) объекта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectKind" type="com:RefBookType">
                <xs:annotation>
                    <xs:documentation>Значение из справочника «Вид объекта»</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectPurpose" type="com:RefBookType">
                <xs:annotation>
                    <xs:documentation>Значение из справочника «Назначение объекта»</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObjectPurposeFunctional" type="com:RefBookType">
                <xs:annotation>
                    <xs:documentation>Значение из справочника «Функциональное назначение объекта»</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BuildCapitalType" type="com:RefBookType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Значение из справочника «Вид объекта капитального строительства»
