	<!--Для валидации запроса/ответа на уровне СМЭВ3-->
	<xs:element name="Request">
		<xs:annotation>
			<xs:documentation>Запрос на предоставление услуг Росреестра в электронном виде</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Тип запроса на предоставление услуг Росреестра в электронном виде</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element name="region" type="das:dRegionsRF">
					<xs:annotation>
						<xs:documentation>
							Код региона
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="externalNumber" type="xs:string">
					<xs:annotation>
						<xs:documentation>
							Номер обращения в информационной системе отправителя
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="senderType" type="req:senderTypes">
					<xs:annotation>
						<xs:documentation>
							Тип информационной системы Потребителя
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="actionCode" type="dAcC:DActionCode" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Код учетно-регистрационного действия (дополнительный, для тестирования в режиме эмуляции)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Attachment" type="req:AttachmentRequestType">
					<xs:annotation>
						<xs:documentation>Блок описания вложения</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Response" type="req:responseType">
		<xs:annotation>
			<xs:documentation>Ответ из Росреестра с номером заявки</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="responseType">
		<xs:annotation>
			<xs:documentation>Тип ответа на предоставление услуг Росреестра в электронном виде</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="requestNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation>Номер обращения во ФГИС ЕГРН</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="State">
				<xs:annotation>
					<xs:documentation>Статус</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="code" type="xs:string" minOccurs="1">
							<xs:annotation>
								<xs:documentation>Код статуса</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="name" type="xs:string" minOccurs="1">
							<xs:annotation>
								<xs:documentation>Наименование</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="StateDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Описание статуса (причина отказа, приостановки текстом при наличии)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StateParameter" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Параметры статуса</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Key" type="xs:string" minOccurs="1">
							<xs:annotation>
								<xs:documentation>Ключ</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Value" type="xs:string" minOccurs="1">
							<xs:annotation>
								<xs:documentation>Значение параметра</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Attachment" type="req:AttachmentResponseType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Блок описания вложения</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="senderTypes">
		<xs:annotation>
			<xs:documentation>Тип ИС отправителя</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="EPGU">
				<xs:annotation>
					<xs:documentation>ЕПГУ</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MFC">
				<xs:annotation>
					<xs:documentation>МФЦ</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BC">
				<xs:annotation>
					<xs:documentation>ИС Бизнес Сообществ</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Vedomstvo">
				<xs:annotation>
					<xs:documentation>ИС ФОИВ, РОИВ, ОМСУ</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Other">
				<xs:annotation>
					<xs:documentation>Другое</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AttachmentRequestType">
		<xs:sequence>
			<!-- Указание способа передачи вложения: через ФХ или методом MTOM-передачи -->
			<xs:choice>
				<!-- Признак передачи вложения через ФХ-->
				<xs:element name="AttachmentFSLink" type="xs:boolean"/>
				<!-- Признак передачи вложения методом MTOM -->
				<xs:element name="IsMTOMAttachmentContent" type="xs:boolean"/>
			</xs:choice>
			<!-- Состав вложения -->
			<!-- Опись пакета и тип учетно-регистрационного действия -->
			<xs:element name="RequestDescription" type="req:TValidatedStructuredAttachmentFormat"/>
			<!-- Заявление или заявления -->
			<xs:element name="Statement" type="req:TValidatedStructuredAttachmentFormat" maxOccurs="unbounded"/>
			<!-- Дополнительные файлы (в т.ч. открепленная ЭЦП) -->
			<xs:element name="File" type="req:TStructuredAttachmentFormat" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AttachmentResponseType">
		<xs:sequence>
			<!-- Указание способа передачи вложения: через ФХ или методом MTOM-передачи -->
			<xs:choice>
				<!-- Признак передачи вложения через ФХ-->
				<xs:element name="AttachmentFSLink" type="xs:boolean">
					<xs:annotation>
						<xs:documentation>Признак передачи вложения через ФХ СМЭВ</xs:documentation>
					</xs:annotation>
				</xs:element>
				<!-- Признак передачи вложения методом MTOM -->
				<xs:element name="IsMTOMAttachmentContent" type="xs:boolean">
					<xs:annotation>
						<xs:documentation>Признак передачи вложения с запросом по протоколу MTOM</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<!-- Описание состава вложения -->
			<xs:element name="ResultFile" type="req:TValidatedStructuredAttachmentFormat" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Описание файла результата</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="File" type="req:TStructuredAttachmentFormat" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Описание дополнительного вложения или файла ЭЦП</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TValidatedStructuredAttachmentFormat">
		<xs:annotation>
			<xs:documentation>Тип файла заявления или результата (выписки)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IsUnstructuredFormat" type="xs:boolean" fixed="false">
				<xs:annotation>
					<xs:documentation>Структурированный элемент или нет: валидировать ли на уровне СМЭВ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsZippedPacket" type="xs:boolean" fixed="true">
				<xs:annotation>
					<xs:documentation>Передается в составе zip-архива</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fileName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Путь до файла относительно корня архива</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TStructuredAttachmentFormat">
		<xs:annotation>
			<xs:documentation>Тип файла, приложенного к заявлению</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IsUnstructuredFormat" type="xs:boolean" fixed="true">
				<xs:annotation>
					<xs:documentation>Структурированный элемент или нет: валидировать ли на уровне ЕСМЭВ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsZippedPacket" type="xs:boolean" fixed="true">
				<xs:annotation>
					<xs:documentation>Передается в составе zip-архива</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fileName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Путь до файла относительно корня архива</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>