{
  "name": "gis-portal",
  "version": "0.0.121",
  "private": true,
  "scripts": {
    "postinstall": "cd .. && husky install portal-ui/.husky",
    "start": "ng serve -c dev --disable-host-check --host \"0.0.0.0\"",
    "start:min": "ng serve --disable-host-check --host \"0.0.0.0\"",
    "build": "npm run build-storybook && ng build",
    "lint": "gherkin-lint && eslint ./src/ && stylelint ./src/**/*.*css",
    "lint:fix": "prettier --write ./src/**/*.{css,scss,ts,tsx} && gherkin-lint && eslint ./src/ --fix && stylelint ./src/**/*.*css --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ./bl",
    "wdio": "cross-env TS_NODE_PROJECT=tests/tsconfig.json wdio",
    "test": "jest",
    "test:unit": "npm test",
    "test:ui": "rm -rf tests/_screens/.tmp/* && npm run wdio -- run tests/ui/wdio.conf.ts",
    "test:ui:dev": "rm -rf tests/_screens/.tmp/* && npm run wdio -- run tests/ui/wdio.dev.conf.ts",
    "test:ui:dev:wip": "npm run test:ui:dev -- --cucumberOpts.tags=\"@wip\"",
    "test:e2e": "rm -rf tests/_screens/.tmp/* && npm run wdio -- run tests/e2e/wdio.conf.ts",
    "test:e2e:dev": "rm -rf tests/_screens/.tmp/* && npm run wdio -- run tests/e2e/wdio.dev.conf.ts",
    "test:e2e:dev:wip": "npm run test:e2e:dev -- --cucumberOpts.tags=\"@wip\"",
    "test:e2e:devLocal": "rm -rf tests/_screens/.tmp/* && npm run wdio -- run tests/e2e/wdio.devLocal.conf.ts",
    "test:e2e:devLocal:wip": "npm run test:e2e:devLocal -- --cucumberOpts.tags=\"@wip\"",
    "test:allureReport": "allure generate --clean allure-results && allure open"
  },
